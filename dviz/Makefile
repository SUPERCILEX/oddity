.PHONY: deploy
deploy:
	npm install
	npm run build
	lein uberjar

## deploy:
## 	wget https://raw.githubusercontent.com/technomancy/leiningen/stable/bin/lein
## 	chmod +x ./lein
## 	./lein
## 	./lein minify-assets
## 	./lein cljsbuild once
## 	./lein deploy
## 	cp -r resources/public/* target/deploy/
